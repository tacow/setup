export PS1='[\u@\h \W]\$ '
export PS2='> '
export PS4='+ '
export GREP_OPTIONS='--color=auto'
export GREP_COLOR='1;31'
export HISTTIMEFORMAT='%F %T  '
alias ls='ls -FG'
alias ll='ls -lFG'
alias lh='ls -lhFG'
alias la='ls -alFG'
alias Df='df -h'
alias Du='du -sh *'
alias vi='vim -p'
alias vim='vim -p'
